<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yujing.yujing.dao.user.problem.UproblemDao" >

    <select id="uselectcountproblem" resultType="java.lang.Integer">
        select count(*) from problem
    </select>
    <select id="uselectproblemlimit" resultType="com.yujing.yujing.pojo.Problem">
      select * from problem limit ${param1},${param2}
    </select>

</mapper>

